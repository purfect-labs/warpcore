      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "pathfinder": {
      "fill": "#45b7d1",
      "stroke": "#333",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "oracle": {
      "fill": "#ffeaa7",
      "stroke": "#333",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "architect": {
      "fill": "#96ceb4",
      "stroke": "#333",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "enforcer": {
      "fill": "#fab1a0",
      "stroke": "#333",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "craftsman": {
      "fill": "#fd79a8",
      "stroke": "#333",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "craftbuddy": {
      "fill": "#e17055",
