{
  "flow_config": {
    "diagram_type": "flowchart TD",
    "title": "WARPCORE Agent Flow Configuration",
    "version": "1.0.0"
  },
  "node_styles": {
    "origin": {
      "fill": "#8b5cf6",
      "stroke": "#6d28d9",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "boss": {
      "fill": "#7c3aed",
      "stroke": "#6d28d9",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "pathfinder": {
      "fill": "#3b82f6",
      "stroke": "#2563eb",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "oracle": {
      "fill": "#f59e0b",
      "stroke": "#d97706",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "architect": {
      "fill": "#22c55e",
      "stroke": "#16a34a",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "enforcer": {
      "fill": "#ef4444",
      "stroke": "#dc2626",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "craftsman": {
      "fill": "#06b6d4",
      "stroke": "#0891b2",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "craftbuddy": {
      "fill": "#84cc16",
      "stroke": "#65a30d",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "gatekeeper": {
      "fill": "#10b981",
      "stroke": "#059669",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "complete": {
      "fill": "#22c55e",
      "stroke": "#16a34a",
      "stroke_width": "2px",
      "shape": "rectangle"
    },
    "user": {
      "fill": "#8b5cf6",
      "stroke": "#6d28d9",
      "stroke_width": "2px",
      "shape": "circle"
    },
    "mama_bear": {
      "fill": "#f97316",
      "stroke": "#ea580c",
      "stroke_width": "2px",
      "shape": "hexagon"
    },
    "harmony": {
      "fill": "#ec4899",
      "stroke": "#db2777",
      "stroke_width": "2px",
      "shape": "diamond"
    }
  },
  "node_templates": {
    "agent_node": "{position}[\"{position}<br/>{name}<br/>{role_short}\"]",
    "user_node": "USER([\"üë§ USER<br/>{description}\"])",
    "external_node": "{id}([\"üåê {name}<br/>{description}\"])"
  },
  "edge_styles": {
    "default": {
      "style": "-->",
      "label_format": "|\"{label}\"|"
    },
    "loop": {
      "style": "-.->",
      "label_format": "|\"{label}\"|",
      "color": "#ff9f43"
    },
    "decision": {
      "style": "==>",
      "label_format": "|\"{label}\"|",
      "color": "#00cec9"
    },
    "external": {
      "style": "-->",
      "label_format": "|\"{label}\"|",
      "color": "#6c5ce7"
    }
  },
  "style_classes": {
    "generate_classes": true,
    "class_definitions": [
      "classDef origin fill:{fill},stroke:{stroke},stroke-width:{stroke_width}",
      "classDef boss fill:{fill},stroke:{stroke},stroke-width:{stroke_width}",
      "classDef pathfinder fill:{fill},stroke:{stroke},stroke-width:{stroke_width}",
      "classDef oracle fill:{fill},stroke:{stroke},stroke-width:{stroke_width}",
      "classDef architect fill:{fill},stroke:{stroke},stroke-width:{stroke_width}",
      "classDef enforcer fill:{fill},stroke:{stroke},stroke-width:{stroke_width}",
      "classDef craftsman fill:{fill},stroke:{stroke},stroke-width:{stroke_width}",
      "classDef craftbuddy fill:{fill},stroke:{stroke},stroke-width:{stroke_width}",
      "classDef gatekeeper fill:{fill},stroke:{stroke},stroke-width:{stroke_width}",
      "classDef complete fill:{fill},stroke:{stroke},stroke-width:{stroke_width}"
    ]
  },
  "flow_patterns": {
    "role_truncation": {
      "max_length": 25,
      "suffix": "..."
    },
    "node_spacing": {
      "vertical_spacing": 2,
      "horizontal_spacing": 3
    },
    "label_formatting": {
      "max_label_length": 30,
      "wrap_labels": true,
      "use_icons": true
    }
  },
  "output_options": {
    "include_styling": true,
    "include_comments": true,
    "compact_format": false,
    "add_title_comment": true,
    "add_watermark_comment": false
  }
}